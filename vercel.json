{
  "routes": [
    {
      "src": "/google/(?<sheetId>.+?)@(?<version>.+)",
      "dest": "/api/google?sheetId=$sheetId&version=$version"
    },
    {
      "src": "/google/(?<sheetId>.+)",
      "dest": "/api/google?sheetId=$sheetId"
    },
    {
      "src": "/phrase/projects$",
      "dest": "/api/phrase-projects"
    },
    {
      "src": "/phrase/projects/(?<projectId>.+)",
      "dest": "/api/phrase-projects?id=$projectId"
    },
    {
      "src": "/phrase/(?<projectId>.+?)@(?<version>.+)",
      "dest": "/api/phrase?projectId=$projectId&version=$version"
    },
    {
      "src": "/phrase/(?<projectId>.+)",
      "dest": "/api/phrase?projectId=$projectId"
    }
  ]
}
